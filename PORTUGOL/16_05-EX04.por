programa {
  real nota1, nota2, nota3, somar
  funcao inicio() {
    escreva ("Digite a nota 1:\n")
    leia (nota1)
escreva ("Digite a nota 2:\n")
    leia (nota2)
escreva ("Digite a nota 3:\n")
    leia (nota3)
    somar = (nota1 + nota2 + nota3)
      se (somar >= 5){
    escreva ("A média é "+ somar +" : Aprovado")
    }senao{
    escreva ("A média é "+ somar + ": Reprovado")}
   

  }
}
